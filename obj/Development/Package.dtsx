          SQLTask:Connection="{31031828-7DB0-4B7E-8A26-CBEB1470786A}"
          SQLTask:SqlStatementSource="INSERT INTO FACT_DICHVUKHAMBENH (&#xA;    IDNhanVien, &#xA;    IDDichVu, &#xA;    IDChiNhanh, &#xA;    IDNgayThang, &#xA;    SoLuotSuDung&#xA;)&#xA;SELECT &#xA;    nhanvien.IDNhanVien,&#xA;    dichvu.IDDichVu,&#xA;    chinhanh.IDChiNhanh,&#xA;    ngaythang.IDNgayThang,&#xA;    1 AS SoLuotSuDung -- Gán giá trị mặc định cho SoLuotSuDung&#xA;FROM &#xA;    DIM_NHANVIEN nhanvien&#xA;    CROSS JOIN DIM_DICHVU dichvu&#xA;    CROSS JOIN DIM_CHINHANH chinhanh&#xA;    CROSS JOIN DIM_NGAYTHANG ngaythang&#xA;WHERE &#xA;    NOT EXISTS (&#xA;        SELECT 1 &#xA;        FROM FACT_DICHVUKHAMBENH fact&#xA;        WHERE fact.IDNhanVien = nhanvien.IDNhanVien&#xA;          AND fact.IDDichVu = dichvu.IDDichVu&#xA;          AND fact.IDChiNhanh = chinhanh.IDChiNhanh&#xA;          AND fact.IDNgayThang = ngaythang.IDNgayThang&#xA;    );&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
